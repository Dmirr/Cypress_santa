{
  "name": "cypress_santa",
  "version": "1.0.0",
  "description": "learning cypress",
  "main": "index.js",
  "scripts": {
    "cy:open": "npx cypress open",
    "cy:run": "cypress run",
    "cy:dashboard": "npx cypress run --record --key 169ee9b6-7890-48a0-8d1d-e28d53b14c3f",
    "cy:dash2":  "npx cypress run --config-file cypress.config.santa.js --record --key 169ee9b6-7890-48a0-8d1d-e28d53b14c3f",
    "cy:spec1":  "npx cypress run --spec 'cypress/e2e/santa.cy.js' --browser chrome",
    "cy:spec2":  "npx cypress run --spec 'cypress/e2e/spec_santa.cy.js' --browser electron",
    "cy:run:all": "npm run cy:spec1 && npm run cy:spec2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dmirr/Cypress_santa.git"
  },
  "keywords": [
    "cypress"
  ],
  "author": "Dmirr",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Dmirr/Cypress_santa/issues"
  },
  "homepage": "https://github.com/Dmirr/Cypress_santa#readme",
  "dependencies": {
    "cypress": "^12.9.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0"
  }
}
